page{
  background: #fff !important;
}

.box {
  padding: 0 30rpx;
}

.box-man {
  display: flex;
  box-shadow: 0px 3px 12px rgba(0, 0, 0, 0.08);
  margin-top: 30rpx;
}

.header {
  width: 202rpx;
  height: 235rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.header image {
  width: 100%;
  height: 100%;
}

.man {
  flex: 1;
  padding: 0 20rpx;
}

.man-1 {
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.man-1>view:nth-child(1) {
  font-size: 34rpx;
  color: #232326;
  font-weight: 500;
}

.man-1>view:nth-child(2) {
  font-size: 30rpx;
  color: #6D7278;
}

.man-2 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #6D7278;
  margin: 10rpx 0;
}

.man-2>view {
  width: 50%;
  text-align: left;
}

.man-3 {
  font-size: 28rpx;
  color: #6D7278;
}

.man-3 span {
  color: #F07067;
  font-size: 36rpx;
  margin-left: 20rpx;
}

.man-where {
  font-style: oblique;
  font-weight: 900;
  font-size: 60rpx;
  color: #ebebeb;
}