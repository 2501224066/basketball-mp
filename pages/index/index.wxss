page {
  background: #fff !important;
}

.top {
  width: 750rpx;
  position: relative;
  overflow: hidden;
}

.bgo {
  width: 1000rpx;
  height: 1000rpx;
  border-radius: 500rpx;
  background: #C6D2FF;
  position: absolute;
  left: -117.5rpx;
  top: -600rpx;
}

swiper {
  margin-top: 7vh;
}

.swiper-item {
  margin-top: 65rpx;
}

.icon-list {
  display: flex;
  align-items: center;
  margin: 40rpx 0;
  padding: 0 4%;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #000;
}

.icon-list image {
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 20rpx;
}

.icon-list>view {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.box {
  padding: 20rpx 30rpx;
  border-top: 20rpx solid #F5F7FA;
}

.box-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}

.label {
  font-size: 34rpx;
  color: #000;
  display: flex;
  align-items: center;
}

.label image {
  width: 44rpx;
  height: 44rpx;
  margin-left: 10rpx;
}

.more {
  font-size: 24rpx;
  color: #999;
}

.box-content {
  box-shadow: 0px 3px 12px rgba(0, 0, 0, 0.08);
  border-radius: 8rpx;
}

.vi {
  width: 100%;
  height: 310rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.vi video,
.vi image {
  width: 100%;
  height: 100%;
}

.box-title {
  color: #333;
  font-size: 32rpx;
  padding: 20rpx;
}

.box-man {
  display: flex;
  box-shadow: 0px 3px 12px rgba(0, 0, 0, 0.08);
  margin-bottom: 20rpx;
}

.header {
  width: 202rpx;
  height: 235rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.header image {
  width: 100%;
  height: 100%;
}

.man {
  flex: 1;
  padding: 0 20rpx;
}

.man-1 {
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.man-1>view:nth-child(1) {
  font-size: 34rpx;
  color: #232326;
  font-weight: 500;
}

.man-1>view:nth-child(2) {
  font-size: 30rpx;
  color: #6D7278;
}

.man-2 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #6D7278;
  margin: 10rpx 0;
}

.man-2>view {
  width: 50%;
  text-align: left;
}

.man-3 {
  font-size: 28rpx;
  color: #6D7278;
}

.man-3 span {
  color: #F07067;
  font-size: 36rpx;
  margin-left: 20rpx;
}

.man-where {
  font-style: oblique;
  font-weight: 900;
  font-size: 60rpx;
  color: #ebebeb;
}