page {
  background: #fff !important;
}

.user {
  padding: 40rpx 30rpx;
  display: flex;
  background: #f3f3f3;
}

.user>image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 65rpx;
  border: 2rpx solid #f3f3f3;
  margin-right: 20rpx;
}

.user>view {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.title {
  font-size: 34rpx;
  color: #000;
  font-weight: 500;
}

.memo {
  font-size: 26rpx;
  color: #999;
  margin-top: 10rpx;
}

.box {
  padding: 0 30rpx;
  margin-top: 30rpx;
}

.team {
  padding: 20rpx;
  border-radius: 8rpx;
  border: 1px solid #EBEBEB;
}

.team-top {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #6D7278;
  font-weight: 500;
}

.team-top>image {
  width: 56rpx;
  height: 56rpx;
  border-radius: 28rpx;
  margin-right: 20rpx;
}

.team-status {
  background: #FDD430;
  color: #fff;
  font-size: 22rpx;
  margin-left: 20rpx;
  padding: 2rpx 10rpx;
  border-radius: 8rpx;
}

.team-body {
  margin-top: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.team-line {
  width: 2rpx;
  height: 50rpx;
  border-right: 1rpx solid #EBEBEB;
}

.team-body>view {
  width: 50%;
  text-align: center;
}

.team-body>view>view:nth-child(1) {
  font-size: 35rpx;
  color: #FDD430;
}

.team-body>view>view:nth-child(2) {
  font-size: 24rpx;
  color: #6D7278;
  margin-top: 20rpx;
}

.op {
  padding: 10rpx 0;
  border-radius: 8rpx;
  border: 1px solid #EBEBEB;
}

.op>view {
  padding: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.op image {
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
}

.op-title {
  display: flex;
  align-items: center;
}

.op-title text{
  font-size: 44rpx !important;
  margin-right: 20rpx;
}

.unlogin {
  margin: 4vh;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 8rpx;
  background: #FDD430;
  color: #333;
  font-weight: 500;
}