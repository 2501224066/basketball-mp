page {
  background: #fff !important;
}

.box {
  padding: 0 30rpx;
}

.list {
  padding: 20rpx;
  margin-top: 20rpx;
  box-shadow: 0px 3px 12px rgba(0, 0, 0, 0.08);
  display: flex;
  align-items: center;
  border-radius: 8rpx;
}

.list image {
  width: 130rpx;
  height: 130rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
}

.list>view {
  flex: 1;
}

.team-title {
  font-size: 34rpx;
  color: #232326;
  font-weight: 500;
}

.team-memo {
  font-size: 28rpx;
  color: #999;
  margin-top: 10rpx;
  font-style: oblique;
}

.team-memo text{
  margin-right: 10rpx;
}