page {
  background: #fff !important;
}

.box {
  padding: 0 30rpx;
}

.list {
  margin-top: 20rpx;
  box-shadow: 0px 3px 12px rgba(0, 0, 0, 0.08);
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 8rpx;
}

.list image {
  width: 203rpx;
  height: 235rpx;
  border-radius: 8rpx;
}

.list-body {
  padding: 10rpx 20rpx;
  flex: 1;
  font-size: 28rpx;
  color: #6D7278;
}

.list-detail {
  display: flex;
  align-items: center;
}

.title {
  font-size: 34rpx;
  color: #232326;
  font-weight: 500;
}

.height {
  font-size: 30rpx;
  color: #F7B52A;
  margin-left: 20rpx;
}

.list-detail-1 {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}

.list-label {
  flex: 1;
  padding-left: 10rpx;
}